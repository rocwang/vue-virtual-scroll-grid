    "rxjs": "^7.5.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.0",
    "@types/ramda": ">=0.28.13",
    "@vitejs/plugin-vue": "^2.3.3",
    "algoliasearch": "^4.13.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.2",
    "lodash-es": "^4.17.21",
    "prettier": "^2.6.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "vite": "^2.9.13",
    "vue": "^3.2.36",
    "vue-tsc": ">=0.38.2"
  },
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "18.4.0",
    "npm": "8.12.2"
  }
