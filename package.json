{
  "name": "vue-virtual-scroll-grid",
  "author": "Roc Wong <roc@kiwiberry.nz> (https://kiwiberry.nz/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rocwang/vue-virtual-scroll-grid.git"
  },
  "keywords": [
    "vue",
    "windowing",
    "virtual scroll",
    "grid"
  ],
  "bugs": {
